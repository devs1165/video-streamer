{
  "name": "stream2peer",
  "version": "1.0.0",
  "main": "bootstrap.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --no-notify -r tsconfig-paths/register src/bootstrap.ts",
    "build": "tsc && resolve-tspaths",
    "prod": "node dist/bootstrap.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/Lib/Infra/Internal/DBContext/Datasource.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/Lib/Infra/Internal/DBContext/Datasource.ts"
  },
  "keywords": [
    "stream",
    "livepeer",
    "oss"
  ],
  "author": "JUMO",
  "license": "ISC",
  "description": "backend->stream2peer",
  "dependencies": {
    "@types/helmet": "^4.0.0",
    "axios": "^1.7.7",
    "backend": "file:",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.4",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "humps": "^2.0.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "livepeer": "^3.4.0",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "mustache": "^4.2.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.15",
    "pg": "^8.13.0",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.2.2",
    "slugify": "^1.6.6",
    "stream2peer": "file:",
    "ts-node": "^10.9.2",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-validator": "^3.0.0",
    "@types/humps": "^2.0.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.10",
    "@types/luxon": "^3.4.2",
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.7.5",
    "@types/nodemailer": "^6.4.16",
    "@types/randomstring": "^1.3.0",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  }
}
